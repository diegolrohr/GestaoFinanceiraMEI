<script>
    function fnUpdateDataFinal() {               
        let date = $$$._.mesPicker.pickamonth("picker").get("select");
        var firstDay = new Date(date.year, date.month, 1);
        let lastDay = new Date(date.year, date.month + 1, 0);
        var dtInicial = $("#dtInicial").val();
        var dtFinal = $("#dtFinal").val();
        if ($("#dtInicial").val() != "") {
            $("#dataInicial").val(moment(dtInicial).format('YYYY-MM-DD'));
            $("#dataFinal").val(moment(dtFinal).format('YYYY-MM-DD'));
        }
        else {
            $("#dataInicial").val(moment(firstDay).format('YYYY-MM-DD'));
            $("#dataFinal").val(moment(lastDay).format('YYYY-MM-DD'));
        }
        $$$.fn.updateActiveMenu("/OrdemServico/List");
        $$$._.fly01dt.refresh();

    }
</script>