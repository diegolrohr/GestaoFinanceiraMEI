<script>
    function fnCancelarNovaOrdem() {        
        var idRecord = $('form#fly01frm input#id').val();
        if (onCancelMustDeleteOrdemServico) {
            $$$.loading.start();
            $.ajax({
                type: "POST",
                url: "@Url.Action("Delete", "OrdemServico")",
                data: { id: idRecord },
                success: function (data) {
                    $$$.loading.stop();
                    if (data.success) {                        
                        $$$.go("@Url.Action("List", "OrdemServico")");
                    }
                    else {
                        $$$.fn.submitErrorHandler(null, data.message);
                    }
                },
                error: function (jXHR, textStatus, errorThrown) {
                    $$$.loading.stop();
                    $$$.toast(textStatus, "error");
                }
            });
        }
        else {
            $$$.go("@Url.Action("List", "OrdemServico")");
        }
    }
</script>