<script>
    function fnShowISS() {
        //Se ha calculo de ISS (checked), habilite os campos relacionados
        if ($('#calculaIss:checked').val() == "true") {
            $('input#retemISS').removeAttr('disabled');
            $('select#tipoTributacaoISS').removeAttr('disabled');
            $('select#tipoTributacaoISS').material_select();
            $('select#tipoPagamentoImpostoISS').removeAttr('disabled');
            $('select#tipoPagamentoImpostoISS').material_select();
            $('select#tipoCFPS').removeAttr('disabled');
            $('select#tipoCFPS').material_select();
            if ($('select#tipoTributacaoISS').val() == null) {// Garante sempre um valor ao habilitar campo
                $('select#tipoTributacaoISS').val("T00");
            }
            if ($('select#tipoPagamentoImpostoISS').val() == null) {// Garante sempre um valor ao habilitar campo
                $('select#tipoPagamentoImpostoISS').val("DentroMunicipio");
            }
            if ($('select#tipoCFPS').val() == null) {// Garante sempre um valor ao habilitar campo
                $('select#tipoCFPS').val("Tomador");
            }
        }
        else {
            //Se nao ha calculo de ISS, desabilite os campos relacionados
            $('input#retemISS').attr('disabled', 'disabled');
            $('select#tipoTributacaoISS').attr('disabled', 'disabled');
            $('select#tipoTributacaoISS').material_select();
            $('select#tipoPagamentoImpostoISS').attr('disabled', 'disabled');
            $('select#tipoPagamentoImpostoISS').material_select();
            $('select#tipoCFPS').attr('disabled', 'disabled');
            $('select#tipoCFPS').material_select();
        }
    }
</script>