<script>
    function fnFormReadyAtualizaIE() {
        $$$.loading.start();
        $.ajax({
            type: "POST",
            url: "@Url.Action("PostAtualizacaoIE", "CertificadoDigital")",
            data: {
                inscricaoEstadual: $('#inscricaoEstadualId').val()
            },
            success: function (response) {
                if (response.success) {
                    $$$._.fly01mdlfrmAtualizaIE.close();
                    fnAtualizaCertificado();                    
                }
                else {
                    fnGetStatusCertificado();
                    $$$.loading.stop();
                    $$$.fn.submitErrorHandler(null, response.message);
                }
            }
        });
    }
</script>
