<script>

    function fnImprimirExtrato() {
        $("#prnt").loading("start");
        $("#dtExtratoDetalhe").on('draw.dt', function () {
            if ($$$._.dtExtratoDetalhe.page.info().length == 500) {
                $("#dtExtratoDetalhe").off('draw.dt');
                $("#prnt").loading("stop");
                window.print();
                $$$._.dtExtratoDetalhe.page.len(50).draw();
            }            
        });
        $$$._.dtExtratoDetalhe.page.len(500).draw();
    }
</script>
