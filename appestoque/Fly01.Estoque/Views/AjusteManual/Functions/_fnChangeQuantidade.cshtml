<script>
    function fnChangeQuantidade() {
        var saldoProduto = parseInt(0 + $("#saldoProduto").val());
        var quantidade = parseInt(0 + $("#quantidade").val())
        var novoEstoque;

        switch ($("#tipoEntradaSaida").val().toLowerCase()) {
            case "entrada":
                novoEstoque = saldoProduto + quantidade;
                break;
            case "saida":
                novoEstoque = saldoProduto - quantidade;
                break;
            default:
                novoEstoque = 0;
                break;
        }

        $("#novoEstoque").val(novoEstoque);
    }
</script>