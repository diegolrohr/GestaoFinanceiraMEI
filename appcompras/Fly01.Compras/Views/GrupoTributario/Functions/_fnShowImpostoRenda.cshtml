<script>
    function fnShowImpostoRenda() {
        //Se ha calculo de ImpostoRenda (checked), habilite os campos relacionados
        if ($('#calculaImpostoRenda:checked').val() == "true") {
            $('input#retemImpostoRenda').removeAttr('disabled');
        }
        else {
            //Se nao ha calculo de ImpostoRenda, desabilite os campos relacionados
            $('input#retemImpostoRenda').attr('disabled', 'disabled');
        }
    }
</script>