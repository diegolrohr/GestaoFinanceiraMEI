<script>
    function fnChangeProdutoDesc(event, data) {
        $('#produtoId').val(data.item.id);
        $('#produtoDescricao').val(data.item.label);
        $('#produtoCodigo').val(data.item.detail);
        $('#saldoProduto').val(data.item.saldo);
        $('#produtoCodigoId').val(data.item.id);
        fly01.core.updateTextFields();
    }

</script>