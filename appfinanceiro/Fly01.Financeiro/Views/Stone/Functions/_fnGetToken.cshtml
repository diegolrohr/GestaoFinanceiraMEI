<script>
    function fnGetToken() {
        if ($("form#fly01frmLogin").valid()) {
            var senha = $("#senha").val();
            $$$.loading.start();
            $.ajax({
                url: '@Url.Action("GetToken", "Stone")',
                type: "POST",
                data: { senha: senha},
                success: function (data) {
                    if (data.success) {
                        $$$.loading.stop();
                         $$$.go('@Url.Action("List", "Stone")');
                    } else {
                        $$$.loading.stop();
                        $$$.toast(data.message, 'error');

                    }
                },
            });
        }
    }
</script>