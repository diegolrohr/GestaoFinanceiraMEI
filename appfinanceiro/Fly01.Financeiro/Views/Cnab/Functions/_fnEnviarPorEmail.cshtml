<script>
    function submitButtonEmail() {
        var attr = { "type": "submit", "class": "btn btn-narrow valid", "id": "" }

        return createElem("button", attr,
            createElem("i", { "class": "material-icons" }, "email")
        );
    };

    function createElement(row) {
        var formAttr = {
            "method": "GET",
            "action": "@Url.Action("Edit", "Cnab")" + "/{" + row.id + "}",
            "class": "cnabFormMail  col s12"
        };
        return createElem("a", formAttr,[
            inputHiddenId("contaReceberId", row.contaReceberId),
            inputHiddenId("contaBancariaId", row.contaBancariaId),
            submitButtonEmail()
        ]);
    };

    function fnEnviarPorEmail(data, type, row, meta) {
        return createElement(row).outerHTML;
    };
</script>