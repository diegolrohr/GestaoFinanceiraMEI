<script>
    function fnChangeTipoInscricao() {
        if ($('#tipoIndicacaoInscricaoEstadual').val() == "ContribuinteIsento") {
            $("#inscricaoEstadual").attr("disabled", true);
            $("#inscricaoMunicipal").attr("disabled", true);
        } else {
            $("#inscricaoEstadual").attr("disabled", false);
            $("#inscricaoMunicipal").attr("disabled", false);
        }
    }

    function fnDesabilitaCamposPorTipoDeInscricao() {
        if ($('#id').val() == "" || $('#id').val() == undefined || $('#tipoIndicacaoInscricaoEstadual').val() == "ContribuinteIsento") {
            $("#inscricaoEstadual").attr("disabled", true);
            $("#inscricaoMunicipal").attr("disabled", true);
        }
    };
</script>
