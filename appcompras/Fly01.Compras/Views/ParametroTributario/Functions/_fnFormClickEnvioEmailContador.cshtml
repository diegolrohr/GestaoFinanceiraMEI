<script>
    function fnFormClickEnvioEmailContador() {
        $$$.loading.start();

        $.ajax({
            url: '@Url.Action("EnviaEmailContador", "ParametroTributario")',
            data: {
                simplesNacional: $('#aliquotaSimplesNacional').val(),
                impostoRenda: $('#aliquotaImpostoRenda').val(),
                csll: $('#aliquotaCSLL').val(),
                cofins: $('#aliquotaCOFINS').val(),
                pisPasep: $('#aliquotaPISPASEP').val(),
                iss: $('#aliquotaISS').val(),
                email: $('#emailId').val(),
                fcp: $('#aliquotaFCP').val(),
                inss: $('#aliquotaINSS').val(),
            },
            type: "POST",
            success: function (data) {
                $$$.loading.stop();
                if (data.success) {
                    $$$._.fly01mdlfrmEnvioEmailContador.close();
                    $$$.toast("Email enviado com sucesso.", "success");
                } else {
                    $$$.fn.submitErrorHandler(null, data.message);
                }
            },
            error: function (jqxhr, textStatus, error) {
                var err = 'Ocorreu um erro na requisição: ' + textStatus + ", " + error;
                $$$.toast(err, "error");
                $$$.loading.stop();
            }
        });
    }



</script>
