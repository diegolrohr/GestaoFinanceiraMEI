            url: '@Url.Action("GerarArquivoRemessa","Cnab")',
            contentType: "application/json",
            data: JSON.stringify({ 'ids': fly01._.dtBoletos.selected }),
            success: function (data) {
                ShowSuccess(data.message);
            }
        });
    };

</script>