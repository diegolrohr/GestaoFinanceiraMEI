<script>
    function fnFormReady() {
        $("#emiteNotaFiscal").click(function () {
            if ($("#emiteNotaFiscal").is(":checked")) {
                $("[data-go*=CertificadoDigital]").each((i, x) => $(x).closest("li").toggleClass("hide", false));
            }
            else {
                $("[data-go*=CertificadoDigital]").each((i, x) => $(x).closest("li").toggleClass("hide", true));
            }
        }); 
    }

    $(function () {
        $$$._.fly01frm.refresh('00000000-0000-0000-0000-000000000000');
    });
</script>

