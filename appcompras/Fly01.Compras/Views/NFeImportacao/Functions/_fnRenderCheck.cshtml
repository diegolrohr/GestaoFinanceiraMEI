<script>
    function fnRenderCheck(row, type, ignoreOuterHTML) {
        let attr = { "type": "checkbox", "class": "permchk", "id": `${type}${row.id}`, "name": `${type}` };
        if (row.parentId) attr["data-parentId"] = row.parentId;
        if (type) attr["data-type"] = type;
        if (row.id) attr["data-id"] = row.id;
        if (row[type]) attr.checked = "checked";

        var element = createElem("div", null, [
            createElem("input", attr),
            createElem("label", { "for": `${type}${row.id}` })
        ])

        if (ignoreOuterHTML)
            return element;

        return element.outerHTML;
        
    }

</script>
