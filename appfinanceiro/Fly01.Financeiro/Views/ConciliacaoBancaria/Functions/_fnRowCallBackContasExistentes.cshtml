<script>
    function fnRowCallbackContasExistentes(row, data) {
        //comportamento padrão
        $("#dtContasExistentes tbody tr").each(function () {
            $(this).find("td").attr("nowrap", "nowrap");
        });
        $("#dtContasExistentes").DataTable().responsive.recalc();
        if ($$$._.dtContasExistentes.selected) {
            if ($.inArray(data.id, $$$._.dtContasExistentes.selected) !== -1) {
                $(row).addClass("selected");
                //comportamento específico, reescrever o valor digitado
                var index = arrayConciliacoes.findIndex(x => x.contaFinanceiraId == data.id);
                if (index !== -1) {
                    $('input.valorParcial', row).val(arrayConciliacoes[index].valorConciliado);
                }
            }
        }
        $$$.fn.maskFields();

        return row;
    };
</script>