<script>
    function fnChangeInput() {
        $("input#dataEmissao").on("change", function () {
            if ($("label[for='dataEmissao']").attr("class") === "truncate") {
                $("#dataInicial").val("");
                $("#dataFinal").val("");
            }
        });

        $("input#dataEntrega").on("change", function () {
            if ($("label[for='dataEntrega']").attr("class") === "truncate") {
                $("#dataInicial").val("");
                $("#dataFinal").val("");
            }
        });
    }
</script>