
<script>
	function fnChangeTipoProduto(event) {
	    $("#grupoProdutoDescricao").data("url-post", "@Url.Action("NovoGrupoProduto", "Produto")?tipo=" + $("#tipoProduto").val());

	    if ($("#tipoProduto").val() == "Insumo")
	        $("#valorVenda").attr('disabled', 'disabled');
        else
	        $("#valorVenda").removeAttr('disabled');
    }
</script>