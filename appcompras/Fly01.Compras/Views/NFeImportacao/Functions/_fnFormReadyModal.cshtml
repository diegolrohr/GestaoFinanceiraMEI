<script>
    function fnFormReadyModal() {
        $("#btnValor").on("click", function () {
            $("input#valorId").removeAttr('disabled');
            $("input#percentualId").attr('disabled','disabled');
            $('#percentualId').val("");
            $(this).data('clicked', true);
            $('#btnPercent').data('clicked', false);
        });

        $("#btnPercent").on("click", function () {
            $("input#percentualId").removeAttr('disabled');
            $("input#valorId").attr('disabled', 'disabled');
            $('#valorId').val("");
            $(this).data('clicked', true);
            $('#btnValor').data('clicked', false);
        });

        $('#btnPercent').click();
    }
</script>