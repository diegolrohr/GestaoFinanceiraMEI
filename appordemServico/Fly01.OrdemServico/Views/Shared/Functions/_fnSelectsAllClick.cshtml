<script>
    function fnSelectsAllClick(btnValue) {
        switch (btnValue) {
            case "selectAll":
                $$$._.dtContasExistentes.rows().select();
                break;
            case "deselectAll":
                $$$._.dtContasExistentes.rows().deselect();
                break;
            default:
                break;
        }
    }
</script>