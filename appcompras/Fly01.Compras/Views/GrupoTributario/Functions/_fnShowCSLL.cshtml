<script>
    function fnShowCSLL() {
        //Se ha calculo de CSLL (checked), habilite os campos relacionados
        if ($('#calculaCSLL:checked').val() == "true") {
            $('input#retemCSLL').removeAttr('disabled');
        }
        else {
            //Se nao ha calculo de CSLL, desabilite os campos relacionados
            $('input#retemCSLL').attr('disabled', 'disabled');
        }
    }
</script>