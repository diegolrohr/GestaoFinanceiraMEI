<script>
    @Html.Partial("Functions/_fnMain");

    function fnFormReady() {
        $("#cpfcnpj").keyup(function () {
            if ($("#cpfcnpj").val() != "" && $("#cpfcnpj").val().length == 11)
                fnDesabilitaCamposPessoaJuridica(true);
            else
                fnDesabilitaCamposPessoaJuridica(false);
        });

        fnDesabilitaCamposPorTipoDeInscricao();

        $("#cpfcnpj").keyup();
        setNFERequiredFields("fly01frm", ["consumidorFinal", "idEstrangeiro", "paisNome"]);
        setNFSERequiredFields("fly01frm", ["situacaoEspecialNFS",]);
        setNFEAndNFSERequiredFields("fly01frm", ["cpfcnpj", "nome", "nomeComercial", "tipoIndicacaoInscricaoEstadual", "inscricaoEstadual", "inscricaoMunicipal", "cep", "estadoNome", "cidadeNome", "bairro", "endereco", "numero"]);
    }

    function fnDesabilitaCamposPessoaJuridica(disabled) {
        $("#nomeComercial").attr("disabled", disabled);
        $("#contato").attr("disabled", disabled);
    }
</script>