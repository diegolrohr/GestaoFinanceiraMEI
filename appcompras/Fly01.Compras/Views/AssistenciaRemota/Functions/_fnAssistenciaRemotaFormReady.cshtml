<script>
    @Html.Partial("Functions/_fnImprimir");

    
    function fnAssistenciaRemotaFormReady() {
        $$$._.instaladorTeamViewer.info("A instalação deste programa poderá ser necessária durante o atendimento da equipe de suporte do Bemacash Gestão Finaceira.<br />");
        $("#instaladorTeamViewer h5").css({ "text-align": "left" });
        $("#instaladorTeamViewer span").css("font-weight", "Bold");
        $("#instaladorTeamViewer h5").addClass("thin");

        $("#instaladorTeamViewer a").on("click", function () {
            fnBaixarArquivo();
        })
    }

    function fnBaixarArquivo() {
        fnImprimir("https://mpn.azureedge.net/suporte/TeamViewer_Setup.exe");
    }
</script>