<script>
    function fnShowINSS() {
        //Se ha calculo de INSS (checked), habilite os campos relacionados
        if ($('#calculaINSS:checked').val() == "true") {
            $('input#retemINSS').removeAttr('disabled');
        }
        else {
            //Se nao ha calculo de INSS, desabilite os campos relacionados
            $('input#retemINSS').attr('disabled', 'disabled');
        }
    }
</script>