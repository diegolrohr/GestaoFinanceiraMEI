<script>
    function fnChangeCheckButton() {
        $("input#emailDestino").attr("disabled", "disabled");

        if (this.id == "notificaViaEmail") {
            if (this.checked) {
                $("input#emailDestino").removeAttr("disabled");
            }
        } 
    }

    function fnChangeCheckButtonSMS() {
        $('input#contatoDestino').attr('disabled', 'disabled');

        if (this.id == "notificaViaSMS") {
            if (this.checked) {
                $("input#contatoDestino").removeAttr("disabled");
            }
        } 
    }
</script>